<body>
    <form action="/qwer" method="post">
        <div class="wrap">
            <div class="form-group">
                <label for="exampleFormControlInput1">제목</label>
                <input type="text" name="title" class="form-control" id="example1" placeholder="">
            </div>
            <div class="form-group">
                <label for="exampleFormControlInput1">작성자명</label>
                <input type="text" name="author" class="form-control" id="example2" placeholder="">
            </div>
            <div class="form-group">
                <label for="exampleFormControlTextarea1">내용</label>
                <textarea class="form-control" name="desc" id="example3" rows="4"></textarea>
            </div>
            <div class="label">
                <label for="exampleFormControlInput1" style="font-weight: bold;">비밀 번호 : </label>
                <input class="inputPwd" name="password" id="borderPwd" type="number" placeholder="비밀번호를 입력하세요" />
            </div>
            <button type="submit" class="btn btn-primary" value="확인">포스팅하기 </button>
        </div>
    </form>
